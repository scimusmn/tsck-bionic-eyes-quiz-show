{
  "name": "app-template",
  "private": true,
  "description": "Basic template for starting a new kiosk or web project",
  "version": "1.0.0",
  "author": "@scimusmn",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "dotenv": "^8.2.0",
    "gatsby": "^2.25.3",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.2.39",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sass": "^2.1.28",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-sharp": "^2.3.13",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "reactstrap": "^8.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/register": "^7.12.1",
    "chalk": "^4.1.0",
    "depcheck": "^1.3.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.3.0",
    "i18next": "^20.3.4",
    "lodash": "^4.17.20",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "react-i18next": "^11.11.3",
    "sort-package-json": "^1.44.0",
    "stylelint": "^13.6.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "clean": "gatsby clean",
    "depcheck": "node -r @babel/register scripts/depcheck.js",
    "develop": "gatsby develop --port 3000",
    "deploy:preview": "yarn build && gsutil -m rsync -d -r public/ gs://tsck-bionic-eyes-quiz-preview.smm.org",
    "install:arduino-base": "node scripts/install-arduino-base.js",
    "lint": "npm-run-all lint:*",
    "lint:code": "node_modules/.bin/eslint .",
    "lint:style": "node_modules/.bin/stylelint \"**/*.*css\"",
    "serve": "gatsby serve --port 3000",
    "pull:assets": "echo Copy contents into local ./static directory. && open https://drive.google.com/drive/folders/1oGcegalN2YYuyDHV5dF3Us88NUfgAphi?usp=sharing",
    "drive-bucket-sync": "node scripts/drive-sync.js",
    "update-media-urls": "node scripts/update-urls.js",
    "save-captions": "node scripts/save-captions.js",
    "save-assets": "node scripts/save-assets.js"
  }
}
